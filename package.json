{
  "name": "tetres",
  "version": "1.0.0",
  "description": "A tetris game written in React.",
  "main": "game.js",
  "scripts": {
    "copyBsCss": "cp node_modules/bootstrap/dist/css/bootstrap.min.css build/",
    "copyCss": "cp src/app.css build && npm run copyBsCss",
    "webpack": "webpack",
    "build": "cp index.html build/ && npm run copyCss && webpack",
    "start": "http-server ./build -a localhost -p 8000 -c-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangchj/tetres.git"
  },
