  "bugs": {
    "url": "https://github.com/wangchj/tetres/issues"
  },
  "homepage": "https://github.com/wangchj/tetres#readme",
  "devDependencies": {
    "http-server": "^0.9.0"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.3.0"
  }
}
